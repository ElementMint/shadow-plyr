{
    "ci": {
      "collect": {
        "startServerCommand": "npx serve demo -l 3000",
        "startServerReadyPattern": "Accepting connections",
        "url": ["http://localhost:3000"],
        "numberOfRuns": 1
      },
      "assert": {
        "preset": "lighthouse:recommended"
      },
      "upload": {
        "target": "temporary-public-storage"
      }
    }
  }
  